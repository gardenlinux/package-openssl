git_src --branch debian/openssl-3.2.2-1 https://salsa.debian.org/debian/openssl.git

## Apply upstream patches
cp -v patches/*.patch $dir/src/debian/patches/
cat patches/series >> $dir/src/debian/patches/series
cat $dir/src/debian/patches/series
cat patches/changelog >> $dir/src/debian/patches/changelog

# set the corresponding version of the package.
version_orig=3.2.3
version="$version_orig-0"
#version_suffix="gardenlinux0+bp1443"
